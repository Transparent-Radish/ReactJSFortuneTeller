//https://www.english-grammar-revolution.com/list-of-prepositions.html
//I had to view the source to copy+paste the words XD
const prepositions = ['aboard', ' about', ' above', ' across', ' after', ' against', ' ahead of', ' along', ' amid', ' amidst', ' among', ' around', ' as', ' as far as', ' as of', ' aside from', ' at', ' athwart', ' atop', 'barring', ' because of', ' before', ' behind', ' below', ' beneath', ' beside', ' besides', ' between', ' beyond', ' but', ' by', ' by means of ', ' concerning', ' despite', ' down', ' during', ' except', ' except for', ' excluding', ' far from', ' following', ' for', ' from', ' in', ' in accordance with', ' in addition to', ' in case of', ' in front of', ' in lieu of', ' in place of', ' in spite of', ' including', ' inside', ' instead of', ' into', ' near', ' next to', ' off', ' on', ' on account of', ' on behalf of', ' on top of', ' onto', ' opposite', ' out', ' out of', ' outside', ' over', ' past', ' plus', ' prior to', ' regarding', ' regardless of', ' through', ' throughout', ' till', ' to', ' toward', ' towards', ' under', ' underneath', ' unlike', ' until', ' up', ' upon', 'versus', ' via', ' with', ' with regard to', ' within', ' without']

const future = ['will', 'shall', 'are going to', 'must']

const articles = ['the', 'a']

const ordinalEndings = ['th','st','nd','rd','th','th','th','th','th','th']

export default class App extends Component {
    state={
       fortune:"Press the button to determine your fate.",
       fortuneCount:0
    }
    newFortune=(structure)=>{
        var s="You "
        
        for(var i=0;i<structure.length;i++){
            s+=' '+structure[i][Math.floor(structure[i].length*Math.random())]
        }
        s+='.'
        this.setState({fortune: s, fortuneCount: this.state.fortuneCount+1})
    }
    
    render() {
        return (
            <View style={styles.container}>
                <View style={styles.titleContainer}>
                    fortune teller
                    
                </View>
                <View style={styles.rowContainer}>
                    <Image
                        source={{ uri: 'https://www.yourmodernfamily.com/wp-content/uploads/2013/01/fortune-cookie-2.jpg' }}
                        style={{ height: deviceHeight/6,width: deviceHeight/6,marginRight: deviceWidth/7}}
                    />
                    <Image
                        source={{ uri: 'https://www.goodfreephotos.com/albums/vector-images/ancient-medieval-scroll.png' }}
                        style={{ height: deviceHeight/6,width: deviceHeight/6}}
                    />
                </View>
                <TouchableHighlight style = {styles.button}
                    onPress={()=>{
                        this.newFortune([future,verbs,prepositions,articles,adjectives,nouns])
                    }}
                    >
                        
                    <Text style = {styles.buttonText}>New Fortune</Text>
                        
                </TouchableHighlight>
                <View style={styles.fortuneContainer}>
                {this.state.fortune}
                </View>
                 <View style={styles.fortuneCountContainer}>
                Fortune #{this.state.fortuneCount}
                </View>
            </View>
            
        );
    }
}

const styles = StyleSheet.create({
    container: {
        height: deviceHeight,
        width: deviceWidth,
        backgroundColor:'#de2910',
        fontFamily:'Comic Sans MS',
        alignItems:'center'
    },
    rowContainer:{
        flexDirection:'row'
    },
    titleContainer:{
        fontSize:deviceWidth/10,
        fontWeight:'bold',
        color:'#ffde00',
        marginBottom:deviceHeight/16,
        marginTop:deviceHeight/32,
    },
    button:{
        height:deviceHeight/16,
        marginTop:deviceHeight/32,
        width:deviceWidth/3,
        backgroundColor:'yellow',
        borderWidth:3,
        borderColor:'dark red',
        alignItems:'center',
        justifyContent:'center'
    },
    buttonText:{
        fontFamily:'Comic Sans MS',
    },
    fortuneContainer:{
        fontFamily:'Comic Sans MS',
        width:4*deviceWidth/5,
        height:4*deviceHeight/10,
        color:'#ffde00',
        fontSize:deviceHeight/20,
        borderWidth:3,
        margin:deviceHeight/32,
        alignItems:'center',
        justifyContent:'center',
    },
    fortuneCountContainer:{
        fontFamily:'Comic Sans MS',
        width:4*deviceWidth/5,
        height:deviceHeight/10,
        color:'#ffde00',
        fontSize:deviceHeight/20,
        borderWidth:3,
        alignItems:'center',
        justifyContent:'center',
    },
});
